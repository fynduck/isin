{
  "name": "fynduck/isin",
  "description": "A Laravel package for storing a validating an ISIN (International Securities Identification Number / ISO 6166)",
  "type": "package",
  "keywords": [
    "isin",
    "iso 6166",
    "iso6166",
    "6166",
    "laravel"
  ],
  "homepage": "https://github.com/fynduck/isin",
  "license": "MIT",
  "authors": [
    {
      "name": "fynduck",
      "email": "stason.k12@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/fynduck/isin/issues",
    "source": "https://github.com/fynduck/isin"
  },
  "require": {
    "php": ">=5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7|^5.0",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Fynduck\\ISIN\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fynduck\\ISIN\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Fynduck\\ISIN\\IsinServiceProvider"
      ]
    }
  },
}
